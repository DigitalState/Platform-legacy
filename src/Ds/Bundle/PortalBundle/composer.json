{
    "name": "digitalstate/platform-portal-bundle",
    "type": "symfony-bundle",
    "description": "DigitalState Portal Bundle",
    "homepage": "https://github.com/DigitalState/Platform-Portal-Bundle.git",
    "keywords": ["Oro"],
    "license": "MIT",
    "authors": [
        {
            "name": "DigitalState, Inc",
            "homepage": "http://www.digitalstate.ca"
        }
    ],
    "require": {
        "php": ">=5.6",
        "oro/platform": "2.0.*",
        "oro/platform-serialised-fields": "2.0.*",
        "knplabs/knp-time-bundle": "dev-master",
        "digitalstate/platform-widget-bundle" : "~0.3.0"
    },
    "autoload": {
        "psr-4": { "Ds\\Bundle\\PortalBundle\\": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
