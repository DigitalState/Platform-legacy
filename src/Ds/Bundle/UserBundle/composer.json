{
    "name": "digitalstate/platform-user-bundle",
    "type": "symfony-bundle",
    "description": "DigitalState User Bundle",
    "homepage": "https://github.com/DigitalState/Platform-User-Bundle.git",
    "keywords": ["Oro", "Communication"],
    "license": "MIT",
    "authors": [
        {
            "name": "DigitalState, Inc",
            "homepage": "http://www.digitalstate.ca"
        }
    ],
    "require": {
        "php": ">=5.6",
        "oro/platform": "2.0.*",
        "oro/platform-serialised-fields": "2.0.*",
        "digitalstate/platform-data-bundle" : "~0.3.0",
        "digitalstate/platform-user-persona-bundle" : "~0.3.0"
    },
    "autoload": {
        "psr-4": { "Ds\\Bundle\\UserBundle\\": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}