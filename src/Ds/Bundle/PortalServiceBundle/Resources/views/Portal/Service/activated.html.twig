{% extends 'DsPortalBundle:Portal:base.html.twig' %}

{% block breadcrumb %}
    <ul class="breadcrumb">
        <li>
            <i class="fa fa-home"></i>
            <a href="{{ path('ds_portal_portal_home_index') }}">
                {% trans %}ds.portal.page.portal.home.index.title{% endtrans %}
            </a>
        </li>
        <li>
            <a href="{{ path('ds_portalservice_portal_service_index') }}">
                {% trans %}ds.portalservice.page.portal.service.index.title{% endtrans %}
            </a>
        </li>
        <li>
            <a href="{{ path('ds_portalservice_portal_service_view', { id: service.id }) }}">
                {{ service.titles|localized_value }}
            </a>
        </li>
        <li class="active">
            {% trans %}ds.portalservice.page.portal.service.activated.title{% endtrans %}
        </li>
    </ul>
{% endblock %}

{% block content %}
    <div class="row">
        <div class="col-lg-12">
            <h1>
                {% trans %}ds.portalservice.page.portal.service.activated.title{% endtrans %}
            </h1>
        </div>
    </div>
    <hr>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-body">
                </div>
            </div>
        </div>
    </div>
{% endblock %}
