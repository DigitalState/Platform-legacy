{
    "name": "digitalstate/platform-communication-bundle",
    "type": "symfony-bundle",
    "description": "DigitalState Platform Communication Bundle",
    "homepage": "https://github.com/DigitalState/Platform-Communication-Bundle.git",
    "keywords": ["Oro", "Communication"],
    "license": "MIT",
    "authors": [
        {
            "name": "DigitalState, Inc",
            "homepage": "http://www.digitalstate.ca"
        }
    ],
    "require": {
        "php": ">=5.6",
        "oro/platform": "1.10.*",
        "digitalstate/platform-admin-bundle" : "~0.2.0",
        "digitalstate/platform-api-bundle" : "~0.2.0",
        "digitalstate/platform-data-bundle" : "~0.2.0",
        "digitalstate/platform-entity-bundle" : "~0.2.0",
        "digitalstate/platform-user-bundle" : "~0.2.0",
        "digitalstate/platform-user-persona-bundle" : "~0.2.0",
        "digitalstate/platform-transport-bundle" : "~0.2.0",
        "digitalstate/platform-widget-bundle" : "~0.2.0"
    },
    "autoload": {
        "psr-4": { "Ds\\Bundle\\CommunicationBundle\\": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}